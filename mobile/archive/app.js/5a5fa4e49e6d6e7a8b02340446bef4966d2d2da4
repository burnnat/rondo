]);var k=new Array();var o=null;var d=null;var e=null,l=null;var a=f[j.stave].clef;var b=null;for(var g=0;g<j.notes.length;g++){var h=j.notes[g];var c=this.getVexflowNote(j.notes[g],{clef:a});if(h.beam=="begin"){o=[c]}else{if(h.beam&&o){o.push(c);if(h.beam=="end"){k.push(new Vex.Flow.Beam(o,true));o=null}}}if(h.tie=="end"||h.tie=="continue"){k.push(new Vex.Flow.StaveTie({first_note:d,last_note:c}))}if(h.tie=="begin"||h.tie=="continue"){d=c}if(h.tuplet){if(e){e.push(c)}else{e=[c];l=h.tuplet}if(e.length==l.num_notes){k.push(new Vex.Flow.Tuplet(e,l));e.forEach(function(i){m.addTickable(i)});e=null;l=null}}else{m.addTickable(c)}if(h.lyric){if(!b){b=new Vex.Flow.Voice(m.time);if(j.time.soft){b.setMode(Vex.Flow.Voice.Mode.SOFT)}b.setStave(m.stave)}b.addTickable(new Vex.Flow.TextNote({text:h.lyric.text,duration:h.duration}))}else{if(b){b.addTickable(new Vex.Flow.GhostNote({duration:h.duration}))}}}Vex.Assert(m.stave instanceof Vex.Flow.Stave,"VexFlow voice should have a stave");return[m,k,b]};Vex.Flow.DocumentFormatter.prototype.getVexflowNote=function(e,c){var f=Vex.Merge({},c);f.keys=e.keys;f.duration=e.duration;if(e.stem_direction){f.stem_direction=e.stem_direction}var b=new Vex.Flow.StaveNote(f);var d=0;if(e.accidentals instanceof Array){e.accidentals.forEach(function(g){if(g!=null){b.addAccidental(d,new Vex.Flow.Accidental(g))}d++})}var a=Vex.Flow.parseNoteDurationString(e.duration).dots;for(var d=0;d<a;d++){b.addDotToAll()}return b};Vex.Flow.DocumentFormatter.prototype.getMinMeasureWidth=function(e){if(!(e in this.minMeasureWidths)){var l=0;var a=this.document.getMeasure(e);var r=a.getStaves().map(function(i){var u=this.createVexflowStave(i,0,0,500);var m=500-(u.getNoteEndX()-u.getNoteStartX());if(m>l){l=m}return u},this);var c=document.createElement("canvas");var b=c.getContext("2d");var h=[];var o=0;a.getParts().forEach(function(i){var u=i.getNumberOfStaves();var m=r.slice(o,o+u);i.getVoices().forEach(function(w){var v=this.getVexflowVoice(w,m)[0];h.push(v);v.ticka