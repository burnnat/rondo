:null}}},init:function(){this.getApplication().sio.on({authorized:this.onAuth,logout:this.onLogout,scope:this})},eachStore:function(a){Ext.Array.forEach(["sketches","parts","staves","measures","voices"],function(b){a.call(this,Ext.getStore(b))})},onAuth:function(){this.eachStore(function(a){a.sync()})},onLogout:function(){this.eachStore(function(a){a.getProxy().clear();a.load()})},onDelete:function(){this.eachStore(function(a){a.remove(a.getRange());a.sync()})},onRefresh:function(){this.eachStore(function(a){console.log(a.getStoreId()+": "+a.getCount());var b=[];a.each(function(c){b.push(c.get("localId"))});console.log("["+b.join(", ")+"]")})},onCreateSketch:function(){var a=this.getEditor();a.setRecord(new Tutti.model.Sketch());if(!a.getParent()){Ext.Viewport.add(a)}a.show()},onSaveSketch:function(b){var a=Ext.getStore("sketches");a.add(b);a.sync()},onViewSketch:function(b,a,c,d){this.getMain().push({xtype:"sketchviewer",title:d.get("title"),sketch:d})}});Ext.define("Rondo.controller.Score",{extend:Ext.app.Controller,config:{control:{keyboard:{keytap:"onKeyTap"}},refs:{keyboard:"keyboard",score:"score"}},onKeyTap:function(a){this.tappedPitch=a.getPitch();if(!this.notePanel){this.notePanel=new Tutti.touch.input.NotePanel({modal:true,hideOnMaskTap:true,listeners:{create:this.onCreate,scope:this}})}this.notePanel.showBy(a,"bc-tc?")},onCreate:function(f){var h=this.getScore();var g=h.getSketch();var d=g.measures().first();var a=d.voices();var e=a.first();var c={keys:[Tutti.Theory.getNoteFromPitch(this.tappedPitch,d.getResolvedKey())],duration:f};if(e){var b=e.getNoteData();b.push(c);e.set("notes",b)}else{e=new Tutti.model.Voice({notes:[c]});e.setStaff(g.parts().first().staves().first());a.add(e)}a.sync();h.refreshBlock(0)}});Ext.application({name:"Rondo",views:["Sketches"],controllers:["Ext.io.Controller","Sketches","Score"],io:{appId:"681c6bef-6a59-42ee-8f88-594b57ffecf6"},icon:{"57":"resources/icons/Icon.png","72":"resources/icons/Icon~ipad.png","114":"resources/icon