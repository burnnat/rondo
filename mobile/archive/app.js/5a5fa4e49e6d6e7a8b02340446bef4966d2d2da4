null;var h=k[e];switch(h.nodeName){case"staves":if(g==0){this.numStaves[j]=parseInt(h.textContent)}break;case"key":f=this.fifthsToKey(parseInt(h.getElementsByTagName("fifths")[0].textContent));break;case"time":f=(h.getElementsByTagName("senza-misura").length>0)?{num_beats:4,beat_value:4,soft:true}:{num_beats:parseInt(h.getElementsByTagName("beats")[0].textContent),beat_value:parseInt(h.getElementsByTagName("beat-type")[0].textContent),soft:true};break;case"clef":var c=parseInt(h.getAttribute("number"));var b=h.getElementsByTagName("sign")[0].textContent;var l=parseInt(h.getElementsByTagName("line")[0].textContent);var a=(b=="G"&&l=="2")?"treble":(b=="C"&&l=="3")?"alto":(b=="C"&&l=="4")?"tenor":(b=="F"&&l=="4")?"bass":null;if(c>0){if(g in this.attributes&&j in this.attributes[g]&&this.attributes[g][j].clef instanceof Array){f=this.attributes[g][j].clef}else{f=new Array(this.numStaves[j])}f[c-1]=a}else{f=a}break;case"divisions":f=parseInt(h.textContent);break;default:continue}if(!(g in this.attributes)){this.attributes[g]=[]}if(!(j in this.attributes[g])){this.attributes[g][j]={}}this.attributes[g][j][h.nodeName]=f}return f};Vex.Flow.Backend.MusicXML.prototype.parseNote=function(d,a){var b={rest:false,chord:false};b.tickMultiplier=new Vex.Flow.Fraction(1,1);b.tuplet=null;Array.prototype.forEach.call(d.childNodes,function(i){switch(i.nodeName){case"pitch":var g=i.getElementsByTagName("step")[0].textContent;var q=parseInt(i.getElementsByTagName("octave")[0].textContent);var m=i.getElementsByTagName("alter")[0];if(m){switch(parseInt(m.textContent)){case 1:g+="#";break;case 2:g+="##";break;case -1:g+="b";break;case -2:g+="bb";break}}b.keys=[g+"/"+q.toString()];break;case"type":var o=i.textContent;b.duration={whole:"1",half:"2",quarter:"4",eighth:"8","16th":"16","32nd":"32","64th":"64","128th":"128","256th":"256"}[o];if(b.rest){b.duration+="r"}break;case"dot":var k=b.duration,e=k.indexOf("r");if(b.rest){k=k.substring(0,e)+"dr"}else{k+="d"}b.duration=k;break;case"duration":var f=new Vex.Flow.Fraction(Vex.Flow.RESOLUTION/4*parseInt(i.textContent),a.divisions).simplify();if(isNaN(f.numerator)||isNaN(f.denominator)){throw new Vex.RERR("InvalidMusicXML","Error parsing MusicXML duration")}if(f.denominator==1){f=f.numerator}b.intrinsicTicks=f;if(!b.duration){b.duration="4"}break;case"time-modification":var j=i.getElementsByTagName("actual-notes")[0];var h=i.getElementsByTagName("normal-notes")[0];if(j&&h){j=parseInt(j.textContent);h=parseInt(h.textContent);if(!(j>0&&h>0)){break}b.tickMultiplier=new Vex.Flow.Fraction(h,j);b.tuplet={num_notes:j,beats_occupied:h}}break;case"rest":b.rest=true;var g=i.getElementsByTagName("display-step")[0];var q=i.getElementsByTagName("display-octave")[0];if(g&&q){b.keys=[g.textContent+"/"+q.textContent]}if(!b.duration){b.duration="1r"}break;case"grace":b.grace=true;break;case"chord":b.chord=true;break;case"voice":var p=parseInt(i.textContent);if(!isNaN(p)){b.voice=p}break;case"staff":var l=parseInt(i.textContent);if(!isNaN(l)&&l>0){b.stave=l-1}break;case"stem":if(i.textContent=="up"){b.stem_direction=1}else{if(i.textContent=="down"){b.stem_direction=-1}}break;case"beam":var s=i.textContent;if(s!="begin"&&s!="continue"&&s!="end"){break}if(b.beam!="continue"){b.beam=s}break;case"lyric":var r=i.getElementsByTagName("text")[0];if(r){r=r.textContent}if(r){b.lyric={text:r}}break;case"notations":Array.prototype.forEach.call(i.childNodes,function(u){switch(u.nodeName){case"tied":var t=u.getAttribute("type");switch(t){case"start":b.tie=(b.tie=="end")?"continue":"begin";break;case"stop":b.tie=(b.tie=="begin")?"continue":"end";break;default:Vex.RERR("BadMusicXML","Bad tie: "+t.toString())}break}});break}});if(b.rest&&!b.keys){var c=a.clef;if(c instanceof Array){c=c[b.stave]}switch(c){case"bass":b.keys=["D/3"];break;case"tenor":b.keys=["A/3"];break;case"alto":b.keys=["C/4"];break;case"treble":default:b.keys=["B/4"];break}}return b};Vex.Flow.Backend.MusicXML.prototype.getAttributes=function(a,f){var b={};for(var c=0;c<=a;c++){if(!(c in this.attributes)){continue}if(!(f in this.attributes[c])){continue}var d=this.attributes[c][f];for(key in d){var g=d[key];if(g instanceof Array){if(!(b[key]&&b[key] instanceof Array)){b[key]=[]}for(var e=0;e<g.length;e++){if(typeof b[key][e]=="undefined"||(typeof g[e]!="undefined"&&g[e]!=null)){b[key][e]=g[e]}}}else{b[key]=g}}}if(!b.time){b.time={num_beats:4,beat_value:4,soft:true}}return b};Vex.Flow.Backend.MusicXML.prototype.fifthsToKey=function(c){for(var b in Vex.Flow.keySignature.keySpecs){var a=Vex.Flow.keySignature.keySpecs[b];if(typeof a!="object"||!("acc" in a)||!("num" in a)){continue}if((c<0&&a.acc=="b"&&a.num==Math.abs(c))||(c>=0&&a.acc!="b"&&a.num==c)){return b}}};if(!Vex.Flow.Backend){Vex.Flow.Backend={}}Vex.Flow.Backend.IR=function(){this.documentObject=null};Vex.Flow.Backend.IR.prototype.parse=function(b){if(!Vex.Flow.Backend.IR.appearsValid(b)){throw new Vex.RERR("InvalidArgument","IR object must be a valid document")}if(typeof b.getNumberOfMeasures=="function"&&typeof b.getMeasure=="function"){var a=b.getNumberOfMeasures();for(var c=0;c<a;c++){b.getMeasure(c)}}this.documentObject=b;this.valid=true};Vex.Flow.Backend.IR.prototype.isValid=function(){return this.valid};Vex.Flow.Backend.IR.appearsValid=function(a){return typeof a=="object"&&a.type=="document"};Vex.Flow.Backend.IR.prototype.getNumberOfMeasures=function(){return this.documentObject.measures.length};Vex.Flow.Backend.IR.prototype.getMeasure=function(a){return new Vex.Flow.Measure(this.documentObject.measures[a])};Vex.Flow.Backend.IR.prototype.getStaveConnectors=function(){if(typeof this.documentObject.getStaveConnectors=="function"){return this.documentObject.getStaveConnectors()}return[]};Vex.Flow.Document=function(b,a){if(arguments.length>0){this.init(b,a)}};Vex.Flow.Document.backends=[Vex.Flow.Backend.IR,Vex.Flow.Backend.MusicXML];Vex.Flow.Document.prototype.init=function(e,c){this.options={};Vex.Merge(this.options,c);this.measures=new Array();if(!e){this.backend=null;retur