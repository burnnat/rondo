.")}this.first_note=a.first_note;this.last_note=a.last_note;return this};Vex.Flow.StaveHairpin.prototype.renderHairpin=function(e){var a=this.context;var b=this.render_options.y_shift+20;var d=e.first_y;if(this.position==Vex.Flow.Modifier.Position.ABOVE){b=-b+30;d=e.first_y-e.staff_height}var c=this.render_options.left_shift_px;var f=this.render_options.right_shift_px;switch(this.hairpin){case Vex.Flow.StaveHairpin.type.CRESC:a.moveTo(e.last_x+f,d+b);a.lineTo(e.first_x+c,d+(this.render_options.height/2)+b);a.lineTo(e.last_x+f,d+this.render_options.height+b);break;case Vex.Flow.StaveHairpin.type.DECRESC:a.moveTo(e.first_x+c,d+b);a.lineTo(e.last_x+f,d+(this.render_options.height/2)+b);a.lineTo(e.first_x+c,d+this.render_options.height+b);break;default:break}a.stroke()};Vex.Flow.StaveHairpin.prototype.draw=function(){if(!this.context){throw new Vex.RERR("NoContext","Can't draw Hairpin without a context.")}var c=this.first_note;var b=this.last_note;var d=c.getModifierStartXY(this.position,0);var a=b.getModifierStartXY(this.position,0);this.renderHairpin({first_x:d.x,last_x:a.x,first_y:c.getStave().y+c.getStave().height,last_y:b.getStave().y+b.getStave().height,staff_height:c.getStave().height});return true};Vex.Flow.StaveHairpin.FormatByTicksAndDraw=function(b,e,d,f,a,c){ppt=e.pixelsPerTick;if(ppt==undefined){throw new Vex.RuntimeError("BadArguments","A valid Formatter must be provide to draw offsets by ticks.")}l_shift_px=ppt*c.left_shift_ticks;r_shift_px=ppt*c.right_shift_ticks;hairpin_options={height:c.height,y_shift:c.y_shift,left_shift_px:l_shift_px,right_shift_px:r_shift_px};new Vex.Flow.StaveHairpin({first_note:d.first_note,last_note:d.last_note},f).setContext(b).setRenderOptions(hairpin_options).setPosition(a).draw()};Vex.Flow.Volta=function(b,c,a,d){if(arguments.length>0){this.init(b,c,a,d)}};Vex.Flow.Volta.type={NONE:1,BEGIN:2,MID:3,END:4,BEGIN_END:5};Vex.Flow.Volta.prototype=new Vex.Flow.StaveModifier();Vex.Flow.Volta.prototype.constructor=Vex.Flow.Volta;Vex.F