placed?f.getExtraLeftPx():b)}if(v==0){a=(props_tmp.displaced?f.getExtraRightPx():a)}}r=f}z.push({line:g.line,pos:k,shiftL:b,shiftR:a,note:f,num:o})}}z.sort(function(B,i){return(i.line-B.line)});var m=0;var h=0;var t=0;var p=0;var l=null;var A=null;for(var x=0;x<z.length;++x){var u=0;var o=z[x].num;var q=z[x].line;var f=z[x].note;var j=z[x].shiftL;var d=z[x].shiftR;var k=z[x].pos;if(q!=l||f!=A){m=c+j;h=v+d}var s=o.getWidth()+e;if(k==Vex.Flow.Modifier.Position.LEFT){o.setXShift(c);u=b+s;t=(u>t)?u:t}else{if(k==Vex.Flow.Modifier.Position.RIGHT){o.setXShift(h);u+=s;p=(u>p)?u:p}}l=q;A=f}this.state.left_shift+=t;this.state.right_shift+=p;return this};Vex.Flow.ModifierContext.prototype.formatFretHandFingers=function(){var c=this.state.left_shift;var v=this.state.right_shift;var w=this.modifiers.frethandfinger;var e=1;if(!w||w.length==0){return this}var z=[];var r=null;var b=0;var a=0;for(var x=0;x<w.length;++x){var o=w[x];var f=o.getNote();var g=f.getKeyProps()[o.getIndex()];var k=o.getPosition();if(f!=r){for(n=0;n<f.keys.length;++n){props_tmp=f.getKeyProps()[n];if(c==0){b=(props_tmp.displaced?f.getExtraLeftPx():b)}if(v==0){a=(props_tmp.displaced?f.getExtraRightPx():a)}}r=f}z.push({line:g.line,pos:k,shiftL:b,shiftR:a,note:f,num:o})}z.sort(function(B,i){return(i.line-B.line)});var m=0;var h=0;var t=0;var p=0;var l=null;var A=null;for(var x=0;x<z.length;++x){var u=0;var o=z[x].num;var q=z[x].line;var f=z[x].note;var j=z[x].shiftL;var d=z[x].shiftR;var k=z[x].pos;if(q!=l||f!=A){m=c+j;h=v+d}var s=o.getWidth()+e;if(k==Vex.Flow.Modifier.Position.LEFT){o.setXShift(c+m);u=c+s;t=(u>t)?u:t}else{if(k==Vex.Flow.Modifier.Position.RIGHT){o.setXShift(h);u=a+s;p=(u>p)?u:p}}l=q;A=f}this.state.left_shift+=t;this.state.right_shift+=p;return this};Vex.Flow.ModifierContext.prototype.formatBends=function(){var e=this.state.right_shift;var d=this.modifiers.bends;if(!d||d.length==0){return this}var c=0;var g=0;var f=this.state.text_line;for(var b=0;b<d.length;++b){var a=d[b];a.setXShift(g);g=a.getWidth();a.setTextLine(f)}this.state.right_shift+=g;this.state.text_line+=1;return this};Vex.Flow.ModifierContext.prototype.formatVibratos=function(){var d=this.modifiers.vibratos;if(!d||d.length==0){return this}var g=this.state.text_line;var e=0;var b=this.state.right_shift-7;var f=this.modifiers.bends;if(f&&f.length>0){g--}for(var c=0;c<d.length;++c){var a=d[c];a.setXShift(b);a.setTextLine(g);e+=a.getWidth();b+=e}this.state.right_shift+=e;this.state.text_line+=1;return this};Vex.Flow.ModifierContext.prototype.formatAnnotations=function(){var f=this.modifiers.annotations;if(!f||f.length==0){return this}var e=this.state.text_line;var d=0;for(var b=0;b<f.length;++b){var a=f[b];a.setTextLine(e);var c=a.getWidth()>d?a.getWidth():d;e++}this.state.left_shift+=c/2;this.state.right_shift+=c/2;return this};Vex.Flow.ModifierContext.prototype.formatArticulations=function(){var d=this.modifiers.articulations;if(!d||d.length==0){return this}var f=this.state.text_line;var e=0;for(var b=0;b<d.length;++b){var a=d[b];a.setTextLine(f);var c=a.getWidth()>e?a.getWidth():e;f+=1.5}this.state.left_shift+=c/2;this.state.right_shift+=c/2;this.state.text_line=f;return this};Vex.Flow.ModifierContext.prototype.preFormat=function(){if(this.preFormatted){return}this.formatNotes().formatDots().formatFretHandFingers().formatAccidentals().formatStrokes().formatStringNumbers().formatArticulations().formatAnnotations().formatBends().formatVibratos();this.width=this.state.left_shift+this.state.right_shift;this.preFormatted=true};Vex.Flow.Accidental=function(a){if(arguments.length>0){this.init(a)}};Vex.Flow.Accidental.prototype=new Vex.Flow.Modifier();Vex.Flow.Accidental.prototype.constructor=Vex.Flow.Accidental;Vex.Flow.Accidental.superclass=Vex.Flow.Modifier.prototype;Vex.Flow.Accidental.prototype.init=function(a){var b=Vex.Flow.Accidental.superclass;b.init.call(this);this.note=null;this.index=null;this.type=a;this.position=Vex.Flow.Modifier.Position.LEFT;this.render_options={font_scale:38,stroke_px: