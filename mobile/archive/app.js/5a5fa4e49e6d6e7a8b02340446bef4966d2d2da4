.DOMParser&&typeof XMLDocument!="undefined"){var c=new window.DOMParser();this.document=c.parseFromString(b,"text/xml")}else{if(window.ActiveXObject&&new window.ActiveXObject("Microsoft.XMLDOM")){this.document=new window.ActiveXObject("Microsoft.XMLDOM");this.document.async="false";this.document.loadXML(b)}else{throw new Vex.RERR("UnsupportedBrowserError","No XML parser found")}}}else{if(b instanceof Document){this.document=b}else{this.valid=false;throw new Vex.RERR("ArgumentError","MusicXML requires XML string or DOM Document object")}}this.documentElement=this.document.documentElement;if(this.documentElement.nodeName!="score-partwise"){throw new Vex.RERR("ArgumentError","VexFlow only supports partwise scores")}var a=0;Array.prototype.forEach.call(this.documentElement.childNodes,function(i){if(i.nodeName=="part-list"){this.parsePartList(i)}else{if(i.nodeName=="part"){var d=0;for(var g=0;g<i.childNodes.length;g++){var h=i.childNodes[g];if(h.nodeName!="measure"){continue}if(!(d in this.measures)){this.measures[d]=new Array()}if(this.measures[d].length!=a){Vex.LogFatal("Part missing measure");this.valid=false;return}if(!(d in this.measureNumbers)){var f=parseInt(h.getAttribute("number"));if(!isNaN(f)){this.measureNumbers[d]=f}}this.measures[d][a]=h;var e=h.getElementsByTagName("attributes")[0];if(e){this.parseAttributes(d,a,e)}d++}if(!(a in this.numStaves)){this.numStaves[a]=1}a++}}},this);var a=0;this.numStaves.forEach(function(d){if(d>1){this.staveConnectors.push({type:"brace",parts:[a],system_start:true})}a++},this);this.valid=true};Vex.Flow.Backend.MusicXML.prototype.parsePartList=function(c){var d=0;var a=null;var b=null;Array.prototype.forEach.call(c.childNodes,function(e){switch(e.nodeName){case"part-group":if(e.getAttribute("type")=="start"){a=[];b=[];Array.prototype.forEach.call(e.childNodes,function(f){switch(f.nodeName){case"group-symbol":if(f.textContent=="bracket"||f.textContent=="brace"){b.push({type:f.textContent,system_start:true})}case"group-barline":