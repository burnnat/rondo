++){if(this.note==l[k]||this.all_voices){a=Vex.Min(a,p[e]);g=Vex.Max(g,p[e])}}}switch(this.type){case Vex.Flow.Stroke.Type.BRUSH_DOWN:var o="vc3";var j=-3;var f=a-(d/2)+10;g+=(d/2);break;case Vex.Flow.Stroke.Type.BRUSH_UP:var o="v11";var j=0.5;var f=g+(d/2);a-=(d/2);break;case Vex.Flow.Stroke.Type.ROLL_DOWN:case Vex.Flow.Stroke.Type.RASQUEDO_DOWN:var o="vc3";var j=-3;var b=this.x_shift+j-2;if(this.note instanceof Vex.Flow.StaveNote){a+=1.5*d;if((g-a)%2!=0){g+=0.5*d}else{g+=d}var f=a-d;var h=g+d+2}else{a+=1.5*d;g+=d;var f=a-0.75*d;var h=g+0.25*d}break;case Vex.Flow.Stroke.Type.ROLL_UP:case Vex.Flow.Stroke.Type.RASQUEDO_UP:var o="v52";var j=-4;var b=this.x_shift+j-1;if(this.note instanceof Vex.Flow.StaveNote){f=d/2;a+=0.5*d;if((g-a)%2==0){g+=d/2}var f=g+0.5*d;var h=a-1.25*d}else{a+=0.25*d;g+=0.5*d;var f=g+0.25*d;var h=a-d}break}if(this.type==Vex.Flow.Stroke.Type.BRUSH_DOWN||this.type==Vex.Flow.Stroke.Type.BRUSH_UP){this.context.fillRect(m+this.x_shift,a,1,g-a)}else{if(this.note instanceof Vex.Flow.StaveNote){for(var k=a;k<=g;k+=d){Vex.Flow.renderGlyph(this.context,m+this.x_shift-4,k,this.render_options.font_scale,"va3")}}else{for(var k=a;k<=g;k+=10){Vex.Flow.renderGlyph(this.context,m+this.x_shift-4,k,this.render_options.font_scale,"va3")}if(this.type==Vex.Flow.Stroke.Type.RASQUEDO_DOWN){h=k+0.25*d}}}Vex.Flow.renderGlyph(this.context,m+this.x_shift+j,f,this.render_options.font_scale,o);if(this.type==Vex.Flow.Stroke.Type.RASQUEDO_DOWN||this.type==Vex.Flow.Stroke.Type.RASQUEDO_UP){this.context.save();this.context.setFont(this.font.family,this.font.size,this.font.weight);this.context.fillText("R",m+b,h);this.context.restore()}};Vex.Flow.Measure=function(a){if(typeof a!="object"){throw new Vex.RERR("ArgumentError","Invalid argument to Vex.Flow.Measure")}if(!a.time||!a.time.num_beats||!a.time.beat_value){throw new Vex.RERR("ArgumentError","Measure must be initialized with nonzero num_beats and beat_value")}this.time=Vex.Merge({},a.time);this.attributes={};if(typeof a.attri